{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "my portfolio build with react",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/*.scss -o src/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all start-js -p watch-css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-js build-css",
    "eject": "react-scripts eject"
  },
  "author": "Osama Fayoni",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "font-awesome": "^4.7.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "progressbar.js": "^1.0.1",
    "rc-progress": "^2.2.6",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
